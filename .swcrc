{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "decoratorsBeforeExport": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "minify": {
      "compress": true,
      "mangle": false
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "keepClassNames": true
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false,
  "isModule": true
}
